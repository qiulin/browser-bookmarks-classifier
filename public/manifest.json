{
  "manifest_version": 3,
  "name": "Bookmark Classifier",
  "version": "1.0.0",
  "description": "Classify browser bookmarks using LLM",
  "permissions": [
    "bookmarks",
    "storage"
  ],
  "background": {
    "service_worker": "background.js",
    "type": "module"
  },
  "action": {
    "default_popup": "popup.html",
    "default_title": "Bookmark Classifier"
  },
  "options_page": "options.html"
}
